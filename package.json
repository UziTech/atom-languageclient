    "@types/mocha": "^5.2.5",
    "@types/node": "14.0.24",
    "@types/sinon": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.5.0",
    "mocha": "^5.2.0",
